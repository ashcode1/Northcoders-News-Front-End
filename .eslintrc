{
  "extends": "airbnb",
  "rules": {
      "react/forbid-prop-types": 0,
      "jsx-a11y/no-static-element-interactions": 0,
      "no-console": "off",
      "max-len": 0,
      "react/jsx-filename-extension": [
          1,
          {
              "extensions": [
                  ".js",
                  ".jsx"
              ]
          }
      ],
      "react/jsx-no-bind": [
          0,
          {
              "ignoreRefs": true,
              "allowArrowFunctions": true,
              "allowBind": true
          }
      ]
  },
  "env": {
    "node": true,
    "jest": true,
    "browser": true,
    "es6": true
  }
}